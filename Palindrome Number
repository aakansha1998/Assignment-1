class Palindrome 
{
    public static void main (String[] args)
    {
    //check this number whether it is Palindrome or not
        int n =121;
        int r,t,sum=0;
        t=n;
        while(n>0){
        r=n%10;
        sum=(sum *10)+r;
        n=n/10;
        }
        if(t==sum)
        System.out.println("Palindrome Number");
        else
        System.out.println("Not Palindrome Number");
    }

}
