class Armstrong 
{
    public static void main (String[] args)
    {
        int n =153;//check this number whether it is Armstrong or not
        int r,t,sum=0;
        t=n;
        while(n>0){
        r=n%10;
        sum=sum+r*r*r;
        n=n/10;
        }
        if(t==sum)
        System.out.println("Armstrong Number");
        else
        System.out.println("Not Armstrong Number");
    }

}
