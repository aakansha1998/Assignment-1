class NPrime{
    public static void main(String a[]){
    int n=15;
    System.out.println("Print Prime numbers upto" +" " +n); 
    for(int i=2;i<=n;i++){
        boolean b= true;
    for(int j=2;j<i-1;j++){
      if(i%j==0)
      {  
      b=false; break;
      }
    }
    if(b==true)
    {
        System.out.println(i);
    }
}
}
} 
